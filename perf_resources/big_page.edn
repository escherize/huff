                                                           [:img {:src "//upload.wikimedia.org/wikipedia/commons/thumb/e/e7/Taste_buds.svg/170px-Taste_buds.svg.png", :decoding "async", :class "thumbimage", :srcset "//upload.wikimedia.org/wikipedia/commons/thumb/e/e7/Taste_buds.svg/255px-Taste_buds.svg.png 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/e/e7/Taste_buds.svg/340px-Taste_buds.svg.png 2x", :data-file-width "640", :data-file-height "1091", :width "170", :height "290"}]]
                                                          [:div {:class "thumbcaption"}

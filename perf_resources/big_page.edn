                                 [:img {:src "//upload.wikimedia.org/wikipedia/commons/thumb/6/6c/Gossuin_de_Metz_-_L%27image_du_monde_-_BNF_Fr._574_fo42_-_miniature.jpg/170px-Gossuin_de_Metz_-_L%27image_du_monde_-_BNF_Fr._574_fo42_-_miniature.jpg", :decoding "async", :class "thumbimage", :srcset "//upload.wikimedia.org/wikipedia/commons/6/6c/Gossuin_de_Metz_-_L%27image_du_monde_-_BNF_Fr._574_fo42_-_miniature.jpg 1.5x", :data-file-width "180", :data-file-height "348", :width "170", :height "329"}]]
